{
    "name": "prooxxy/magento2-composer-constraints",
    "description": "A Magento 2 plugin that adds relevancy to the composer.json files in app/code",
    "type": "composer-plugin",
    "version": "2.0.0",
    "keywords": ["magento2", "composer"],
    "license": ["MIT"],
    "require": {
        "composer-plugin-api": "^2.0",
        "php": ">=7.3"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "phpunit/phpunit": "~6.5.0"
    },
    "authors": [
        {
            "name": "Hampus Westman",
            "email": "hampus.westman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PrOOxxy\\MagentoComposerConstraints\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PrOOxxy\\MagentoComposerConstraints\\Test\\": "test/"
        }
    },
    "extra": {
        "class": "\\PrOOxxy\\MagentoComposerConstraints\\Event\\PostInstallCompatability"
    }
}
